<% if Helpers.logged_in?(session) %>
<%= @error %>
  <form action='/tweets' method="post">
    Content:
    <input type="text" name="content"><br>
    Create Your Tweet:

    <input type="submit" name="submit" id="submit">

  </form>

<% else %>
    <h1> You Must Be Logged in to Edit Tweets </h1>
<% end %>
